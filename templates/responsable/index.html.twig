{% extends 'base.html.twig' %}

{% block title %}Mes véhicules{% endblock %}

{% block body %}
<div class="form-group">
    <div class="row">
        <div class="col-12">
            <label for="exampleSelect1">Selectionner votre véhicule</label>
            <br>
            {% for voiture in voitures %}
